FRONTEND_SECRET=123

FRONTEND_API_URL=https://api
FRONTEND_API_CLIENT_SECRET=api-frontend-key

FRONTEND_OAUTH2_ENABLED=false
FRONTEND_OAUTH2_CLIENT_ID=0
FRONTEND_OAUTH2_CLIENT_SECRET=abc

FRONTEND_SESSION_COOKIE_SECURE=false

FRONTEND_ROLE=front
FRONTEND_SESSION_REDIS_DSN=redis://redisfront
FRONTEND_SESSION_MEMCACHE=memcachefront

FRONTEND_TEST_ENABLED=true
FRONTEND_ENABLE_BEHAT_DEBUGGER=1
FRONTEND_GA_DEFAULT=UA-57312200-1
FRONTEND_GA_GDS=

FRONTEND_BEHAT_CONTROLLER_ENABLED=true

FRONTEND_EMAIL_DOMAIN=digideps.dsd.io

FRONTEND_NONADMIN_HOST=https://digideps.local
FRONTEND_ADMIN_HOST=https://admin.digideps.local

# use simple alpha and underscore for bucketname. Fakes3 failing otherwise
FRONTEND_S3_BUCKETNAME=pa-uploads-local
FRONTEND_S3_LOCALSTACK=true

FRONTEND_FILESCANNER_URL=https://file-scanner-api:8443
FRONTEND_FILESCANNER_SSLVERIFY=false

FRONTEND_EMAIL_REPORT_TO=behat-digideps@digital.justice.gov.uk
FRONTEND_EMAIL_FEEDBACK_TO=digideps+feedback@digital.justice.gov.uk
FRONTEND_EMAIL_UPDATE_TO=behat-digideps+update-contact@digital.justice.gov.uk

OPG_DOCKER_TAG=latest
OPG_NGINX_INDEX=app_dev.php

OPG_NGINX_SERVER_NAMES=localhost 127.0.0.1 frontend
OPG_NGINX_REDIRECT_SSL=TRUE
OPG_NGINX_ROOT=/app/web

# needed for file upload
OPG_NGINX_CLIENTMAXBODYSIZE=0
# timeout. 5 mins will allow 15Mb upload on a 512 Kbps connection
OPG_NGINX_CLIENTBODYTIMEOUT=300s

# localstack config
DD_S3_ENDPOINT=http://localstack:4572

VIRTUAL_HOST=www

# sets xdebug values in PHP ini config when OPG_PHP_XDEBUG_ENABLED is set
OPG_PHP_XDEBUG_ENABLED=true
OPG_PHP_XDEBUG_REMOTE_HOST=docker.for.mac.localhost
OPG_PHP_XDEBUG_REMOTE_PORT=9001
OPG_PHP_XDEBUG_IDEKEY=PHPSTORM
