# admin consumes the frontenv.env and overrides it with these settings

API_CLIENT_SECRET=api-admin-key
ROLE=admin
SESSION_REDIS_DSN=redis://redis-frontend
SESSION_PREFIX=dd_session_admin

NGINX_APP_NAME=admin

# sets xdebug values in PHP ini config when OPG_PHP_XDEBUG_ENABLED is set
OPG_PHP_XDEBUG_ENABLED=true
OPG_PHP_XDEBUG_REMOTE_HOST=host.docker.internal
OPG_PHP_XDEBUG_REMOTE_PORT=9001
OPG_PHP_XDEBUG_IDEKEY=PHPSTORM
# enabled cachegrind captures
OPG_PHP_XDEBUG_CACHEGRIND_ENABLED=false
