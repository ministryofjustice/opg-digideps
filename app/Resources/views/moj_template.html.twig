    {% block stylesheets %}{% endblock %}

    {% block head %}{% endblock %}

  </head>

  <body class="{% block bodyClasses %}{% endblock %}">
            {% if env == 'staging' %}
                    <script>
                     (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                     (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                     m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                     })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
                     ga('create', 'UA-57312200-2', 'auto');
                     
                       {% if app.user %}
                            ga('set', '&uid','{{app.user.gaTrackingId()}}');
                       {% endif %} 
                     ga('send', 'pageview');
                   </script>
          {% elseif env == 'develop' %}
                    <script>
                      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
                      ga('create', 'UA-57312200-1', 'auto');

                      {% if app.user %}
                          ga('set', '&uid','{{app.user.gaTrackingId()}}');  
                      {% endif %} 
                      ga('send', 'pageview');
                    </script>
           {% endif %}
    <script type="text/javascript">document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

    <div id="skiplink-container">
      <div>
        <a href="#content" class="skiplink">Skip to main content</a>
      </div>
    </div>

    <div id="global-cookie-message">
      <div class="outer-block">
        <div class="inner-block">


        {% block cookieMessage %}{% endblock %}


        </div>
      </div>
    </div>
    <!--end global-cookie-message-->


    <header role="banner" id="global-header" class="with-proposition {% block headerClass %}{% endblock %}
">
      <div class="header-wrapper">
        <div class="header-global">
          <div class="header-logo">
            <a href="https://www.gov.uk/" title="Go to the GOV.UK homepage" id="logo" class="content">
              <img src="{{ asset('images/gov.uk_logotype_crown.png') }}" width="35" height="31" alt=""> GOV.UK
            </a>
          </div>
            {% block insideHeader %}{% endblock %}

        </div>

        <div class="header-proposition">
          <div class="content">
           {% block proposition_content %}{% endblock %}
          </div>
        </div>


      </div>
    </header>
    <!--end header-->


    <!--[if lte IE 6]><script>document.body.className = document.body.className.replace('js-enabled','');</script><![endif]-->
    {% block afterHeader %}{% endblock %}


    <div id="global-header-bar">
      <div class="inner-block">
        <div class="header-bar"></div>
      </div>
    </div>
    <!--end global-header-bar-->


    {% block beforeContent %}{% endblock %}

    <div id="wrapper" class="group">
      <section id="content" role="main">
       {% block content %}{% endblock %}
      </section>
    </div>

    <footer class="group js-footer" id="footer" role="contentinfo">

      <div class="footer-wrapper">
       {% block footerTop %}{% endblock %}

        <div class="footer-meta">
          <div class="footer-meta-inner">
            <h2 class="visuallyhidden">Support links</h2>
            <ul>
              {% block footerSupportLinks %}{% endblock %}

              <li>
                Built by
                  <a href="https://www.gov.uk/government/organisations/office-of-the-public-guardian">the Office of the Public Guardian</a>
              </li>
            </ul>


            <div class="open-government-licence">
              <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence</a></p>
              <p>All content is available under the <a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence v3.0</a>, except where otherwise stated</p>
            </div>
          </div>

          <div class="copyright">
            <a href="https://www.nationalarchives.gov.uk/information-management/our-services/crown-copyright.htm">&copy; Crown copyright</a>
          </div>
        </div>
      </div>
    </footer>

    <!--end footer-->
    
    <div id="global-app-error" class="app-error hidden"></div>

    <script src="{{ asset('javascripts/govuk-template.js') }}" type="text/javascript"></script>

    <!--[if gt IE ]><!-->
    <script src="{{ asset('javascripts/vendor/jquery/dist/jquery.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('javascripts/moj.js') }}" type="text/javascript"></script>

    {% block javascripts %}{% endblock %}

    <script type="text/javascript">$(moj.init);</script>
    <!--<![endif]-->

    {% block bodyEnd %}{% endblock %}

    {% block popups %}{% endblock %}
    
  </body>
</html>
