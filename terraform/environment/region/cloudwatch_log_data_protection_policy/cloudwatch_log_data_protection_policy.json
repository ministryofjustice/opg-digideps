{
  "Name": "",
  "Version": "2021-06-01",
  "Configuration": {
    "CustomDataIdentifier": [
      {
        "Name": "ClientLastname",
        "Regex": "clientLastname.{5}"
      },
      {
        "Name": "DeputyFirstname",
        "Regex": "deputyFirstname\":\".{5}"
      },
      {
        "Name": "DeputyPostcode",
        "Regex": "deputyPostcode\\\":\\\".{5}"
      }
    ]
  },
  "Statement": [
    {
      "Sid": "audit-policy",
      "DataIdentifier": [
        "arn:aws:dataprotection::aws:data-identifier/EmailAddress",
        "arn:aws:dataprotection::aws:data-identifier/Name",
        "ClientLastname",
        "DeputyFirstname",
        "DeputyPostcode"
      ],
      "Operation": {
        "Audit": {
          "FindingsDestination": {}
        }
      }
    },
    {
      "Sid": "redact-policy",
      "DataIdentifier": [
        "arn:aws:dataprotection::aws:data-identifier/EmailAddress",
        "arn:aws:dataprotection::aws:data-identifier/Name",
        "ClientLastname",
        "DeputyFirstname",
        "DeputyPostcode"
      ],
      "Operation": {
        "Deidentify": {
          "MaskConfig": {}
        }
      }
    }
  ]
}
